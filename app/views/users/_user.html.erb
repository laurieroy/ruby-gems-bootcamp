<b>Id:</b>
<%= link_to user.id, user_path(user) %>
<br >
<b>Email:</b>
<%= user.email %>
<br >
<b>Confirmed:</b>
<%= user.confirmed? %>
<br />
<b><%= 'sign_in_count'.humanize %>: </b>
<%= user.sign_in_count %>
<br />
<b><%= 'sign_in_at'.humanize %>: </b>
<%=  user.current_sign_in_at %>
<br />
<b><%= 'last_sign_in'.humanize %>: </b>
<%=  user.last_sign_in_at %>
<br />
<b><%= 'current_sign_in_ip'.humanize %>: </b>
<%=  user.current_sign_in_ip %>
<br />
<b><%= 'last_sign_in_ip'.humanize %>: </b>
<%=  user.last_sign_in_ip %>
<br>
<b><%= 'locked_at'.humanize %>: </b>
<%=  user.locked_at %>
<br>
<b><%= 'name'.humanize %>: </b>
<%=  user.name %>
<br>
<b><%= 'provider'.humanize %>: </b>
<%=  user.provider %>
<br>
<b><%= 'image'.humanize %>: </b>
<% if user.image.present? %>
  <%= image_tag user.image, size: "50x50" %>
<% end %>
<br>
<b><%= 'uid'.humanize %>: </b>
<%=  user.uid %>
<br>
<b><%= 'roles'.humanize %>: </b>
<%=  user.active_roles.join(",") %>
<br>
<% if current_user.admin? %>
  <%= link_to "Edit roles", edit_user_path(user), class: "text-warning"  %>
  <%= link_to ban_stauts(user), ban_user_path(user), method: :patch, class: "text-warning"  %>
  <%= link_to "Delete", user, method: :delete, data: {confirm: "Are you sure?"}, class: "text-danger"  %>
<% end %>