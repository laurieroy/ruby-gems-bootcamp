All Users(<%= @users.size %>):
<% @users.each do |user| %>
<br />
  <%= render 'users/user', user: user %>
<% end %>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user %></td>
      </tr>
    <% end %>
  </tbody>
</table>